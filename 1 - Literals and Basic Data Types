using System;

class Question1
{
    static void Main()
    {
        float temperature = 36.6f;
        if (temperature > 37.0)
            Console.WriteLine("Fever");
        else
            Console.WriteLine("Normal");

        int age = 18;
        if (age >= 18)
            Console.WriteLine("Eligible to Vote");
        else
            Console.WriteLine("Not Eligible");
        char gender = 'M';
        if (gender == 'M')
            Console.WriteLine("Male");
        else if (gender == 'F')
            Console.WriteLine("Female");
        else
            Console.WriteLine("Other");

        double price = 1000.0;
        double discount = 10.0;
        double finalPrice = price - (price * (discount / 100));
        Console.WriteLine($"Final Price: â‚¹{finalPrice}");

        bool loginSuccess = true;
        if (loginSuccess)
            Console.WriteLine("Login successful");
        else
            Console.WriteLine("Access denied");

        int age2 = 25;
        float temp = 98.6f;
        char grade = 'A';
        bool isPassed = true;

        Console.WriteLine($"age = {age2}, Type = {age2.GetType()}");
        Console.WriteLine($"temperature = {temp}, Type = {temp.GetType()}");
        Console.WriteLine($"grade = {grade}, Type = {grade.GetType()}");
        Console.WriteLine($"isPassed = {isPassed}, Type = {isPassed.GetType()}");
    }
}
